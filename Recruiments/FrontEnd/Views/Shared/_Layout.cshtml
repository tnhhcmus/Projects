

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>iPhoneGiaSiXachTay.Com - @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/js")
</head>
<body>
    <div id="body">
        @RenderSection("featured", required: false)

        <section class="container">
            @Html.Partial("_PageHeader", FrontEnd.Utilities.GetSiteConfig())
            @RenderBody()

            @Html.Partial("_SiteContact", FrontEnd.Utilities.GetSiteConfig());
        </section>

    </div>
    <footer>
        <div>
            <p>&copy; @DateTime.Now.Year by DungTin Solution (dt-solution.vn)</p>
        </div>
    </footer>

    

    
    @RenderSection("scripts", required: false)
</body>
</html>
